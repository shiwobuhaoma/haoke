<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <!--<item name="colorPrimary">@color/colorPrimary</item>-->
        <!--<item name="colorPrimaryDark">@color/colorPrimaryDark</item>-->
        <!--<item name="colorAccent">@color/colorAccent</item>-->
    </style>

    <style name="ShuiBoWen" parent="@style/AppTheme">
        <item name="colorControlHighlight">@color/colorPrimary</item>
        <!--<item name="colorPrimary">@color/colorPrimary</item>-->
        <!--<item name="colorPrimaryDark">@color/colorPrimaryDark</item>-->
        <!--<item name="colorAccent">@color/colorAccent</item>-->
    </style>

    <style name="AppTheme.ToolbarPopupOverlay" parent="ThemeOverlay.AppCompat.Light">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>

    </style>

    <style name="login" parent="@style/AppTheme">
        <item name="android:windowTranslucentStatus">true</item>
        <item name="colorControlHighlight">@color/themeBlue</item>
        <item name="colorAccent">@color/themeBlue</item>
        <item name="colorPrimary">@color/black</item>
        <item name="colorPrimaryDark">@color/black</item>

    </style>

    <style name="AboutUs" parent="AppTheme.AppBarOverlay">
        <item name="windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
    </style>

    <style name="AppTheme.NoActionBar.PopupOverlay" parent="ThemeOverlay.AppCompat">
        <item name="android:windowActionBar">false</item>

    </style>

    <style name="main_menu_animstyle">
        <item name="android:windowEnterAnimation">@anim/photo_dialog_in_anim</item>
        <item name="android:windowExitAnimation">@anim/photo_dialog_out_anim</item>
    </style>

    <style name="top_menu_animstyle">
        <item name="android:windowEnterAnimation">@anim/top_dialog_in_anim</item>
        <item name="android:windowExitAnimation">@anim/top_dialog_out_anim</item>
    </style>

    <style name="transparentFrameWindowStyle" parent="android:style/Theme.Dialog">
        <item name="android:windowBackground">@drawable/photo_choose_bg</item>
    </style>

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="MaterialCalendarTextStyelNormal">

        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#333333</item>

    </style>
    <style name="MaterialCalendarWeekTextStyel">

        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#333333</item>

    </style>

    <style name="nodata_bluebuttonbottom">
        <item name="android:textSize">@dimen/res_textsize_15sp</item>
        <item name="android:textColor">@color/blue_text_007dff</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/no_wifi_setting_button</item>
    </style>


</resources>
